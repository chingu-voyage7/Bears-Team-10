{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "husky": "^1.2.1",
    "koa-static": "^5.0.0",
    "lint-staged": "^8.1.0",
    "prettier": "^1.15.3",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-redux": "^5.1.1",
    "react-scripts": "2.1.1",
    "redux": "^4.0.1",
    "redux-saga": "^0.16.2"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json,scss,css}": [
      "prettier --config .prettierrc --write",
      "git add"
    ]
